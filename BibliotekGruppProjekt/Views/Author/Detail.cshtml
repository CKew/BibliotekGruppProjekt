@model BibliotekGruppProjekt.Models.Author.AuthorDetailModel


<h3>@Model.Name</h3>
<div id="authorDetail">
    <table class="mdl-data-table mdl-js-data-table mdl-data-table--selectable mdl-shadow--2dp customTable" id="patronIndexTable">
        <thead>
            <tr>
                <th>Books: </th>
            </tr>
        </thead>
        <tbody>

            <tr>
                @foreach (var book in Model.Books)
                {
                    <td>@book.Title</td>
                }
            </tr>
        </tbody>
    </table>
    </div>